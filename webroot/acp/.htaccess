<IfModule mod_rewrite.c>
# fixme: users who choose run names that clash with filenames, dont get to access runs
    RewriteEngine On
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME}.php !-d
    RewriteCond %{REQUEST_FILENAME}.php !-f
  	RewriteRule ^([A-Za-z0-9_]+)/?$ edit_run.php?run_name=$1 [QSA,L]
	
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME}.php !-d
    RewriteCond %{REQUEST_FILENAME}.php !-f
  	RewriteRule ^([A-Za-z0-9_]+)/([A-Za-z0-9_]+)/?$ $2.php?run_name=$1 [QSA,L]
	
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^/?([A-Za-z0-9_]+)/?$ $1.php [QSA,L]
	# on HU subdomain
#	RewriteBase /survey/acp/
</IfModule>