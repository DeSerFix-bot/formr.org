{
  "name": "formr",
  "version": "0.6.8",
  "main": ["main.js", "main.css"],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "git://github.com/twbs/bootstrap",
    "font-awesome": "git://github.com/FortAwesome/Font-Awesome",
    "jquery": "git://github.com/components/jquery.git#2.*",
    "select2": "git://github.com/ivaynberg/select2.git#3.*",
    "webshim": "git://github.com/aFarkas/webshim.git#1.*"
  },
  "preen": {
      "jquery": [
        "bower.json",
        "jquery.js",
        "jquery.min.js"
      ],
      "bootstrap": [
          "bower.json",
          "dist/js/bootstrap.js",
          "dist/js/bootstrap.min.js",
          "dist/css/bootstrap.css",
          "dist/css/bootstrap.min.css"
      ],
      "ace-builds": [
          "bower.json",
          "src-noconflict/**",
          "src-min-noconflict/**"
      ],
      "fastclick": [
          "bower.json",
          "lib/*"
      ],
      "font-awesome": [
          "bower.json",
          "css/*",
          "fonts/*"
      ],
      "webshim": [
          "bower.json",
          "js-webshim/**"
      ]
   }
}
